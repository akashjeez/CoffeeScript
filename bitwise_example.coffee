a = 2 # Bit presentation 10
b = 3 # Bit presentation 11
console.log "Value of a is "+a+", b is "+b

result = a & b
console.log "The result of (a & b) is "+ result

result = a | b
console.log "The result of (a | b) is "+ result

result = a ^ b
console.log "The result of (a ^ b) is "+ result

result = ~b
console.log "The result of (~b) is "+ result

result = a << b
console.log "The result of (a << b) is "+ result

result = a >> b
console.log "The result of (a >> b) is "+ result