a = 33; b = 10; c = "test"
console.log "Value of a is "+a+", b is "+b+" and c is "+ c 

result = a + b
console.log "The value of a + b is " + result

result = a - b
console.log "The value of a - b is "+ result

result = a / b
console.log "The value of a / b is "+ result

result = a % b
console.log "The value of a % b is "+ result

result = a + b + c
console.log "The value of a + b + c is "+ result

a = ++a
result = ++a
console.log "The value of ++a is "+ result

b = --b
result = --b
console.log "The value of --b is "+ result